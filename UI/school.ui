        <property name="styleSheet">
         <string notr="true">/* Основной стиль календаря */
QCalendarWidget {
background-color: #ffffff; /* Белый фон */
border: 2px solid #c4c4c4; /* Серые границы */
border-radius: 20px; /* Скруглённые углы */
font-size: 14px; /* Размер шрифта */
color: #333333; /* Цвет текста */
}

QCalendarWidget QWidget {
    background-color: #d9e6f2; /* Новый цвет фона */
    border: none;             /* Убираем рамку */
}

/* Заголовки месяцев и кнопки навигации */
QCalendarWidget QToolButton {
background-color: #98AFE1; /* Фон кнопок (месяц, навигация) */
color: white; /* Белый текст */
border: 1px solid #7289bb; /* Граница кнопок */
border-radius: 4px; /* Скруглённые углы */
font-weight: bold; /* Жирный шрифт */
padding: 5px;
}

QCalendarWidget QToolButton:hover {
background-color: #5c6e96; /* Цвет кнопки при наведении */
}

QCalendarWidget QToolButton:pressed {
background-color: #4b587a; /* Цвет кнопки при нажатии */
}


/* Ячейки с днями месяца */
QCalendarWidget QAbstractItemView:item {
border: 1px dashed #7289bb ; /* Без границ по умолчанию */

}

/* Текущая дата */
QCalendarWidget QAbstractItemView:item:selected {
background-color: #7289bb; /* Цвет выделения текущей даты */
color: white; /* Белый текст */
font-weight: bold;
}


/* Даты из других месяцев */
QCalendarWidget QAbstractItemView:item:!enabled {
background-color: #f5f5f5; /* Светлый фон для дат из других месяцев */
color: #b0b0b0; /* Серый текст */
}

/* Убираем прозрачность */
QCalendarWidget QAbstractItemView {
background: transparent;
}


/* Даты из других месяцев */
QCalendarWidget QAbstractItemView:item:!enabled {
    background-color: #f0f0f0; /* Светло-серый фон */
    color: #a0a0a0;            /* Бледно-серый текст */
    font-style: italic;        /* Курсив для визуального выделения */
}

/* Субботы */
QCalendarWidget QAbstractItemView:item:enabled:nth-of-type(6) {
    background-color: #FFF4E0; /* Пастельно-оранжевый фон */
    color: #CC6600;            /* Оранжевый текст */
    font-weight: bold;         /* Жирный шрифт */
}

/* Воскресенья */
QCalendarWidget QAbstractItemView:item:enabled:nth-of-type(7) {
    background-color: #FFE6E6; /* Пастельно-красный фон */
    color: #CC0000;            /* Красный текст */
    font-weight: bold;         /* Жирный шрифт */
}


